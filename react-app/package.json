{
  "name": "underlator",
  "version": "1.0.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@lingui/core": "^5.2.0",
    "@lingui/react": "^5.2.0",
    "@reduxjs/toolkit": "^2.5.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/react-redux": "^7.1.34",
    "@vitejs/plugin-react": "^4.3.4",
    "@xenova/transformers": "^2.17.2",
    "pixelperfect-tool": "^1.9.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-pdf": "^9.1.1",
    "react-redux": "^9.2.0",
    "react-resize-detector": "^12.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "sass": "^1.70.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.0"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "stylelint": "stylelint '**/*{.css,.scss}'",
    "stylelint:fix": "stylelint '**/*{.css,.scss}' --fix",
    "prettier": "prettier --write .",
    "lint-staged": "lint-staged",
    "prepare": "cd ../ && husky ./react-app/.husky",
    "electron": "npm run build && node electron-build.mjs && cd ../electron-app && npm run make",
    "extract": "lingui extract",
    "compile": "lingui compile --typescript"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@feature-sliced/eslint-config": "^0.1.1",
    "@lingui/babel-plugin-lingui-macro": "^5.2.0",
    "@lingui/cli": "^5.2.0",
    "@lingui/format-po": "^5.2.0",
    "@lingui/vite-plugin": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "cpy": "^11.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-boundaries": "^4.2.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-lingui": "^0.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.34.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.10",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "stylelint": "^16.8.2",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.2",
    "stylelint-scss": "^6.5.0",
    "stylelint-selector-bem-pattern": "^4.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm run lint:fix",
    "*{.css,.scss}": "npm run stylelint:fix",
    "*.{js,jsx,ts,tsx,scss,md,html,json}": "npm run prettier"
  }
}
