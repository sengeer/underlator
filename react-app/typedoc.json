{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src"],
  "out": "../docs/react-app",
  "name": "Underlator React App",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "theme": "default",
  "sort": ["source-order"],
  "kindSortOrder": [
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Variable",
    "Function",
    "Accessor"
  ],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Components",
    "Hooks",
    "Utils",
    "Types",
    "Constants",
    "Other"
  ],
  "searchInComments": true,
  "cleanOutputDir": true,
  "exclude": [
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**"
  ],
  "externalPattern": ["**/node_modules/**"],
  "excludeNotDocumented": false,
  "excludeNotDocumentedKinds": ["Module"],
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  },
  "skipErrorChecking": true
}
